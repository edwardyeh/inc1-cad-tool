{
    "hide_row_parse": false,
    "table_format": 
    {
        "active_ws": "Pinmux",
        "function": 
        {
            "rid": 2, 
            "pattern": ["Function \\d+"]
        },
        "pad_name": 
        {
            "rid": 4,
            "pattern": "Pad Name"
        },
        "ref_name": 
        {
            "rid": 4,
            "pattern": "Pad Cell Name"
        }
    },
    "function": 
    {
        "SDIO": 
        {
            "subgroup": [ {"pattern": "SDIO(\\d*)_.+", "name": "SDIO\\1"} ],
            "clock":    [ "SDIO\\d*_CLK.*" ],
            "custom":   { "DATA_H": "SDIO\\d*_D(?:0|1).*" }
        },
        "BMC":
        {
            "subgroup": [ {"pattern": "BMC_\\w+", "name": "BMC"} ],
            "clock":    [ "\\w+_CLK" ],
            "custom":   {}
        },
        "JTAG":
        {
            "subgroup": [ {"pattern": "JTAG_\\w+", "name": "JTAG"} ],
            "clock":    [ "\\w+_TCK" ],
            "custom":   {}
        }
    },
    "ignore":
    {
        "FUNC": "FUNC_\\w+",
        "FUNC2": "FUNC2_\\w+"
    },
    "partition":
    {
        "top1": ["MC\\d+", "PGPIO\\d+"]
    }
}
